{
  "version": "1.17.1",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run form-runner:install && npm run prisma:generate && webpack",
    "deploy-test": "git push origin HEAD:deploy-test --force",
    "environment": "docker-compose pull && docker-compose up --remove-orphans",
    "build:prod": "NODE_ENV=production npm run form-runner:install && NODE_ENV=production webpack",
    "start:prod": "NODE_ENV=production nodemon ./dist/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest --collect-coverage --detectOpenHandles --forceExit",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "npx prisma generate --schema ./src/server/models/db/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema ./src/server/models/db/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema ./src/server/models/db/schema.prisma --force",
    "prisma:deploy": "npx prisma migrate deploy --schema ./src/server/models/db/schema.prisma",
    "prepare:husky": "husky install && npx husky add .husky/pre-commit \"npx lint-staged\" && npx husky add .husky/pre-push \"npm run type-check\"",
    "form-runner:install": "./src/server/components/formRunner/install-form-runner.sh",
    "form-runner:start": "cd ./lib/form-runner && yarn runner start",
    "version:patch": "npm --no-git-tag-version version patch",
    "version:minor": "npm --no-git-tag-version version minor",
    "version:major": "npm --no-git-tag-version version major"
  },
  "dependencies": {
    "@prisma/client": "^2.29.1",
    "accessible-autocomplete": "^2.0.3",
    "aws-sdk": "^2.972.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.6.2",
    "express-session": "^1.17.2",
    "express-winston": "^4.1.0",
    "govuk-frontend": "^3.13.0",
    "helmet": "^4.6.0",
    "ioredis": "^4.27.9",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "notifications-node-client": "^5.1.0",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "pg-escape": "^0.2.0",
    "prisma": "^2.29.1",
    "promise-polyfill": "8.2.0",
    "whatwg-fetch": "^3.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/body-parser": "^1.19.1",
    "@types/cheerio": "^0.22.30",
    "@types/compression": "^1.7.1",
    "@types/connect-redis": "^0.0.17",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/express-http-proxy": "^1.6.2",
    "@types/express-session": "^1.17.4",
    "@types/express-winston": "^4.0.0",
    "@types/ioredis": "^4.27.5",
    "@types/jest": "^27.0.1",
    "@types/jest-axe": "^3.5.2",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.6.2",
    "@types/node-fetch": "^2.5.12",
    "@types/nunjucks": "^3.1.5",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.1",
    "@types/pg-escape": "^0.2.1",
    "@types/supertest": "^2.0.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "cheerio": "^1.0.0-rc.10",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-axe": "^5.0.1",
    "jest-extended": "^0.11.5",
    "jest-mock-extended": "^1.0.18",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.2.0",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "nodemon-webpack-plugin": "^4.5.2",
    "postcss-loader": "^6.1.1",
    "prettier": "2.3.2",
    "redis-mock": "^0.56.3",
    "sass": "^1.38.0",
    "sass-loader": "^12.1.0",
    "supertest": "^6.1.6",
    "ts-loader": "^9.2.5",
    "ts-standard": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  }
}

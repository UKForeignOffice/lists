{
  "version": "1.0.7",
  "engines": {
    "node": ">=0.14"
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack",
    "start:dev": "NODE_ENV=development nodemon",
    "start:prod": "NODE_ENV=production nodemon",
    "build": "npm run prisma:generate && NODE_ENV=production webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "type-check": "tsc --noEmit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "jest --collect-coverage",
    "test:watch": "jest --watch",
    "prisma:generate": "npx prisma generate --schema ./src/server/models/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema ./src/server/models/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema ./src/server/models/schema.prisma --force",
    "prisma:deploy": "npx prisma migrate deploy --schema ./src/server/models/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^2.21.0",
    "@types/node-fetch": "^2.5.10",
    "accessible-autocomplete": "^2.0.3",
    "aws-sdk": "^2.886.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-rate-limit": "^5.2.6",
    "express-winston": "^4.1.0",
    "govuk-frontend": "^3.11.0",
    "helmet": "^4.4.1",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "nunjucks": "^3.2.3",
    "pg": "^8.6.0",
    "prisma": "^2.21.0",
    "sqlstring": "^2.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.28",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/nunjucks": "^3.1.4",
    "@types/pg": "^7.14.11",
    "@types/sqlstring": "^2.3.0",
    "@types/supertest": "^2.0.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "cheerio": "^1.0.0-rc.6",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.4.1",
    "nodemon": "^2.0.7",
    "postcss-loader": "^5.2.0",
    "prettier": "2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "supertest": "^6.1.3",
    "ts-loader": "^8.1.0",
    "ts-standard": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.32.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-node-externals": "^2.5.2",
    "winston-papertrail": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run type-check",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*{ts,tsx}": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": "eslint --fix",
    "*.js": "eslint --cache --fix"
  }
}

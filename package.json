{
  "version": "1.2.2",
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run form-runner:install && npm run prisma:generate && webpack",
    "build:prod": "NODE_ENV=production npm run form-runner:install && NODE_ENV=production webpack",
    "start:prod": "NODE_ENV=production nodemon ./dist/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest --collect-coverage",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "npx prisma generate --schema ./src/server/models/db/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema ./src/server/models/db/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema ./src/server/models/db/schema.prisma --force",
    "prisma:deploy": "npx prisma migrate deploy --schema ./src/server/models/db/schema.prisma",
    "prepare:husky": "husky install && npx husky add .husky/pre-commit \"npx lint-staged\" && npx husky add .husky/pre-push \"npm run type-check\"",
    "form-runner:install": "./src/form-runner/install-form-runner.sh",
    "form-runner:start": "cd ./lib/form-runner && yarn runner start"
  },
  "dependencies": {
    "@prisma/client": "^2.22.1",
    "@types/node-fetch": "^2.5.10",
    "@types/pg-escape": "^0.2.1",
    "accessible-autocomplete": "^2.0.3",
    "aws-sdk": "^2.908.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^9.0.2",
    "express": "^4.16.3",
    "express-http-proxy": "^1.6.2",
    "express-rate-limit": "^5.2.6",
    "express-winston": "^4.1.0",
    "govuk-frontend": "^3.12.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "notifications-node-client": "^5.1.0",
    "nunjucks": "^3.2.3",
    "pg": "^8.6.0",
    "pg-escape": "^0.2.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.13.16",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.28",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.11",
    "@types/express-http-proxy": "^1.6.1",
    "@types/express-rate-limit": "^5.1.1",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash": "^4.14.169",
    "@types/node": "^15.3.0",
    "@types/nunjucks": "^3.1.4",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "cheerio": "^1.0.0-rc.9",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "nock": "^13.0.11",
    "nodemon": "^2.0.7",
    "nodemon-webpack-plugin": "^4.5.2",
    "postcss-loader": "^5.2.0",
    "prettier": "2.3.0",
    "prisma": "^2.22.1",
    "sass": "^1.32.12",
    "sass-loader": "^11.1.1",
    "supertest": "^6.1.3",
    "ts-loader": "^9.1.2",
    "ts-standard": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0",
    "winston-papertrail-transport": "^1.0.9"
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,css,scss,ts,tsx,md}": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  }
}

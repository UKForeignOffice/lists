{
  "version": "1.4.2",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run form-runner:install && npm run prisma:generate && webpack",
    "build:prod": "NODE_ENV=production npm run form-runner:install && NODE_ENV=production webpack",
    "start:prod": "NODE_ENV=production nodemon ./dist/server.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "jest --collect-coverage --detectOpenHandles --forceExit",
    "test:dev": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "npx prisma generate --schema ./src/server/models/db/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema ./src/server/models/db/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema ./src/server/models/db/schema.prisma --force",
    "prisma:deploy": "npx prisma migrate deploy --schema ./src/server/models/db/schema.prisma",
    "prepare:husky": "husky install && npx husky add .husky/pre-commit \"npx lint-staged\" && npx husky add .husky/pre-push \"npm run type-check\"",
    "form-runner:install": "./src/form-runner/install-form-runner.sh",
    "form-runner:start": "cd ./lib/form-runner && yarn runner start"
  },
  "dependencies": {
    "@prisma/client": "^2.24.0",
    "@types/node-fetch": "^2.5.10",
    "@types/pg-escape": "^0.2.1",
    "accessible-autocomplete": "^2.0.3",
    "aws-sdk": "^2.919.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-redis": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.6.2",
    "express-session": "^1.17.2",
    "express-winston": "^4.1.0",
    "govuk-frontend": "^3.12.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.1",
    "notifications-node-client": "^5.1.0",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-escape": "^0.2.0",
    "redis": "^3.1.2",
    "whatwg-fetch": "^3.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-typescript": "^7.13.0",
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.29",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/express-http-proxy": "^1.6.1",
    "@types/express-session": "^1.17.3",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^26.0.22",
    "@types/jest-axe": "^3.5.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.2",
    "@types/nunjucks": "^3.1.4",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/pg": "^8.6.0",
    "@types/redis": "^2.8.29",
    "@types/supertest": "^2.0.11",
    "babel-plugin-module-resolver": "^4.1.0",
    "cheerio": "^1.0.0-rc.9",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "file-loader": "^6.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.3",
    "jest-axe": "^4.1.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "nock": "^13.1.0",
    "nodemon": "^2.0.7",
    "nodemon-webpack-plugin": "^4.5.2",
    "postcss-loader": "^5.3.0",
    "prettier": "2.3.0",
    "prisma": "^2.24.0",
    "redis-mock": "^0.56.3",
    "sass": "^1.34.0",
    "sass-loader": "^12.0.0",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.2",
    "ts-standard": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0",
    "winston-papertrail-transport": "^1.0.9"
  },
  "lint-staged": {
    "*.{js,json,yml,yaml,css,scss,ts,tsx,md}": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  }
}

{
    "name": "lists-scheduler",
    "version": "0.0.0-development",
    "private": true,
    "engines": {
      "node": ">=14"
    },
    "scripts": {
      "scheduler:batch": "node dist/scheduler/batch.js",
      "scheduler:worker": "node dist/scheduler/worker.js",
      "scheduler:unpublish": "npm run prisma:deploy && node dist/scheduler/unpublish.js",
      "prisma:deploy": "npx prisma migrate deploy --schema src/server/models/db/schema.prisma",
      "prisma:generate": "npx prisma generate --schema src/server/models/db/schema.prisma",
      "task:resendRequestedEditEmail": "node dist/scheduler/run resendRequestedEditEmail",
      "task:unpublishWeeklyTask": "node dist/scheduler/run unpublishWeeklyTask",
      "task:unpublishDayBeforeTask": "node dist/scheduler/run unpublishDayBeforeTask",
      "task:unpublishDayTask": "node dist/scheduler/run unpublishDayTask",
      "task:deleteItemsAfterAYear": "node dist/scheduler/run deleteItemsAfterAYear"
    }
}

